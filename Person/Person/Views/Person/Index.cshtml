@model IEnumerable<Person.Models.PersonModel><link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">@{    ViewBag.Title = "Index";}<br /><p>    <a class="btn btn-success" href="/Person/CreateUpdate"><i class="bi bi-plus-circle"></i>&nbsp;New</a></p>@using (Html.BeginForm("GetFilterData", "Person", FormMethod.Get)){    //@Html.TextBox("search")<input type="text" name="searchText" /><input type="submit" name="Search" value="Search">}<table class="table">    <tr>        <th>            @Html.DisplayNameFor(model => model.Id)        </th>        <th>            @Html.DisplayNameFor(model => model.FullName)        </th>        <th>            @Html.DisplayNameFor(model => model.Gender)        </th>        <th>            @Html.DisplayNameFor(model => model.Province)        </th>        <th>            @Html.DisplayNameFor(model => model.DateOfBirth)        </th>        <th>            @Html.DisplayNameFor(model => model.Email)        </th>        @*<th>                        @Html.DisplayNameFor(model => model.Password)            </th>*@        <th>            @Html.DisplayNameFor(model => model.Title)        </th>        <th>            @Html.DisplayNameFor(model => model.Image)        </th>        <th>Action</th>    </tr>    @foreach (var item in Model)    {<tr>    <td>        @Html.DisplayFor(modelItem => item.Id)    </td>    <td>        @Html.DisplayFor(modelItem => item.FullName)    </td>    <td>        @Html.DisplayFor(modelItem => item.Gender)    </td>    <td>        @Html.DisplayFor(modelItem => item.Province)    </td>    <td>        @Html.DisplayFor(modelItem => item.DateOfBirth)    </td>    <td>        @Html.DisplayFor(modelItem => item.Email)    </td>    @*<td>             @Html.HiddenFor(modelItem => item.Password)        </td>*@    <td>        @Html.DisplayFor(modelItem => item.Title)    </td>    <td>        @Html.DisplayFor(modelItem => item.Image)    </td>    @*<td>        <img src="/Models/RetrieveImage/@item.Image" alt="" height=100 width=200 />    </td>*@    @*<td>                @Html.ActionLink("Edit", "Create", new { id = item.Id }) |                @Html.ActionLink("Details", "Details", new { id = item.Id }) |                @Html.ActionLink("Delete", "Delete", new { id = item.Id })        </td>*@    <td>        @{            var edit = "/Person/CreateUpdate/" + item.Id;            var details = "/Person/Details/" + item.Id;            var delete = "/Person/Delete/" + item.Id;        }        <a class="btn btn-primary" href="@edit"><i class="bi bi-pencil-square"></i>&nbsp;Edit</a>        <a class="btn btn-info" href="@details"><i class="bi bi-archive"></i>&nbsp;Details</a>        <a class="btn btn-danger" href="@delete"><i class="bi bi-trash3"></i>&nbsp;Delete</a>    </td></tr>}</table>